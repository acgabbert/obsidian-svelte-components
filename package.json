{
	"name": "obsidian-svelte-components",
	"version": "0.10.0",
	"description": "Svelte components for Obsidian plugin development.",
	"author": "acgabbert",
	"license": "MIT",
	"scripts": {
		"dev": "npm run build:watch",
		"build": "npm run package",
		"preview": "vite preview",
		"build:watch": "svelte-kit sync && svelte-package --watch",
		"link": "npm update && npm link obsidian-cyber-utils && npm run build",
		"link:watch": "npm update && npm link obsidian-cyber-utils && npm run build",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"browser": "./dist/index.js",
			"import": "./dist/index.js",
			"module": "./dist/index.js",
			"main": "./dist/index.js",
			"svelte": "./dist/index.js",
			"require": "./dist/index.js",
			"default": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/acgabbert/obsidian-svelte-components.git"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"cyber",
		"svelte"
	],
	"dependencies": {
		"marked": "^14.1.3",
		"obsidian-cyber-utils": "latest",
		"tesseract.js": "4.1.1"
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^9.6.0",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"publint": "^0.2.0",
		"svelte": "^4.2.7",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.11",
		"obsidian": "^1.8.7"
	},
	"type": "module"
}
